{
  "name": "vue2-element-admin",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "cross-env webpack serve --env=development --progress --config build/webpack.dev.conf.js",
    "build": "cross-env webpack --env=production --progress --config build/webpack.prod.conf.js",
    "preview": "node build/preview.js",
    "analyzer": "webpack-bundle-analyzer --port 8888 dist/stats.json",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml",
    "prettier": "prettier \"{src,mock}/**/*.{vue,js}\" --write",
    "lint:js": "eslint \"{src,mock}/**/*.{vue,js}\" ",
    "lint:js-fix": "eslint \"{src,mock}/**/*.{vue,js}\" --fix",
    "lint:css": "stylelint \"**/*.{vue,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:css-fix": "stylelint \"**/*.{vue,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/ --fix",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test"
    }
  },
  "lint-staged": {
    "*.{js,vue}": [
      "npm run prettier",
      "npm run lint:js-fix",
      "npm run lint:css-fix",
      "git add ."
    ]
  },
  "dependencies": {
    "axios": "0.27.2",
    "echarts": "5.1.2",
    "element-ui": "2.15.3",
    "js-cookie": "2.2.1",
    "normalize.css": "8.0.1",
    "nprogress": "0.2.0",
    "screenfull": "5.1.0",
    "vue": "2.6.12",
    "vue-router": "3.5.1",
    "vuex": "3.6.2"
  },
  "devDependencies": {
    "@babel/core": "7.13.16",
    "@babel/preset-env": "7.13.15",
    "@vue/test-utils": "1.1.4",
    "autoprefixer": "8.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "26.6.3",
    "babel-loader": "8.2.2",
    "cache-loader": "4.1.0",
    "clean-webpack-plugin": "3.0.0",
    "connect": "3.7.0",
    "cross-env": "7.0.3",
    "css-loader": "5.2.0",
    "eslint": "7.24.0",
    "eslint-plugin-vue": "7.9.0",
    "file-loader": "6.2.0",
    "friendly-errors-webpack-plugin": "1.7.0",
    "glob": "7.1.6",
    "html-webpack-plugin": "5.3.1",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "lint-staged": "10.5.4",
    "mini-css-extract-plugin": "1.4.1",
    "mockjs": "1.0.1-beta3",
    "node-notifier": "9.0.1",
    "node-sass": "5.0.0",
    "optimize-css-assets-webpack-plugin": "5.0.4",
    "path-browserify": "1.0.1",
    "portfinder": "1.0.28",
    "postcss-loader": "5.2.0",
    "prettier": "2.2.1",
    "purgecss-webpack-plugin": "4.0.3",
    "sass": "1.32.10",
    "sass-loader": "11.0.1",
    "stylelint": "13.12.0",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-standard": "21.0.0",
    "stylelint-order": "4.1.0",
    "svg-sprite-loader": "6.0.5",
    "svgo": "1.2.0",
    "thread-loader": "3.0.1",
    "url-loader": "4.1.1",
    "vue-jest": "4.0.1",
    "vue-loader": "15.9.6",
    "vue-style-loader": "4.1.3",
    "vue-template-compiler": "2.6.12",
    "webpack": "5.31.0",
    "webpack-bundle-analyzer": "4.4.0",
    "webpack-cli": "4.9.0",
    "webpack-dev-server": "3.11.2",
    "webpack-merge": "5.7.3"
  }
}
